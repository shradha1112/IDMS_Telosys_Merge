
/*  tab style for exam   */
.exam-tab-style{
    font-size: 11px;
    padding:8px 7px;
    font-weight: 450;
    border: 1px solid #1d7fb7; 
}

/* style for quill editor inner template  */
.innerTemplateStyle>div>strong {
    font-size: 17px !important;
}

/* style for collpase panel  */
div[id^=change-collapse]:after {
    content: '\FE3D';      
    color: #1d7fb7;
    font-weight: bold;
    font-size:25px;
    margin-top:-5px;
    float: left;
    margin-left: 5px;
}
     
.active3:after {
    content: "\FE3E" !important;
    margin-top:7px !important;   
}

/*quill editor tempate style */
.static-panel-style{
    padding-top:20px;
    width:99%;
    padding-left:11px
}
.input-size{
    font-weight: 400;
    color: white;
  }
.header-style{
    color: white;
    font-weight: 600;
    }

.header-contents{
    background-color: #03659d;
    margin-right:17px !important;
    margin-left:17px !important;
    box-shadow: 5px 8px 38px  #888888;
  }
  
  .header-style{
    color: white;
    font-weight: 600;
    }

input::-webkit-input-placeholder {         
    color: #808080 !important;        
}

/* style for exam tab for max screen 768px */
@media screen and (max-width: 768px) {
    .exam-tab-style{
        font-size: 8px !important;
        white-space:pre-wrap;
    }
}

/* style for vital card */
.vital-card-style{
 width:500px !important;
}

