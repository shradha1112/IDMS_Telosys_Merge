<div class="main-content">
    <div class="container-fluid">
        <div class="wizard-container">
            <div class="card wizard-card" data-color="blue" id="wizardProfile">
                <div class="card-header" data-background-color>
                    <div class="row">
                        <div class="col-sm-6 col-md-5  col-xs-5">
                            <tr>
                                <td>
                                    <h6>Update Patient</h6>
                                </td>
                            </tr>
                        </div>
                    </div>
                </div>
                <ngx-spinner bdColor="transparent" loadingText="Loading" size="defaut" color="#03659D"
                fullScreen="true" type="line-scale-pulse-out-rapid">
                </ngx-spinner>
                <div class="card-content ">
                    <div *ngIf="statusCode === 409" class="alert alert-danger" data-icon-bg-color="alert-red">
                        <strong>Patient Name already exists!</strong>
                    </div>
                    <form [formGroup]="updatePatientForm" action="" method="">
                        <div class="row">
                            <div class="col-md-2" style="right:-50px;">
                                <div class="picture-container">
                                    <div class="picture">
                                        <img src={{patientImage}} class="picture-src" id="wizardPicturePreview" title="" />
                                        <input type="file" id="wizard-picture" name="PatientImageFile" (change)="selectFile($event)">
                                        <input type="hidden" class="form-control" id="patientImageId" value="init">
                                    </div>
                                    <h6 style="font-size:12px;">Choose Picture</h6>
                                    <ehr-field-error-display [displayError]="isNotValidImageSize" errorMsg="Upload image less than 30KB.">
                                    </ehr-field-error-display>
                                </div>
                            </div>
                            <div class="col-md-10">
                                <div class="row adjust-content">
                                 <div class="col-xs-1 col-md-1 col-sm-6" style="top:5px">
                                        <div class="form-group">
                                            <label #titlefloatLable id="title_floatToLabel" class="control-label">
                                                <small class="AsteriskColor">*</small> Title
                                            </label>
                                            <md2-select name="title"  formControlName="title" 
                                                style="width: 100%">

                                                <md2-option class="decrease-height" *ngFor="let title of allTitles" [value]="title.lookupCode">{{title.lookupCode}}</md2-option>
                                            </md2-select>
                                            <div class="error-display-margin">
                                                <ehr-field-error-display [displayError]="isFieldValid(updatePatientForm, 'title')" errorMsg="Required">
                                                </ehr-field-error-display>
                                            </div>
                                        </div>
                                    </div>
                                  <div class="col-xs-6 col-md-3 col-sm-6 ">
                                        <div class="form-group">
                                            <label class="control-label"><small class="AsteriskColor">* </small>Last
                                                Name
                                            </label>
                                            <input type="text" name="lastName" formControlName="lastName" class="form-control">
                                            <ehr-field-error-display [displayError]="isFieldValid(updatePatientForm, 'lastName')"
                                                errorMsg="Last Name is required ">
                                            </ehr-field-error-display>
                                        </div>
                                    </div>
                                    
                                    <div class="col-xs-6 col-md-4 col-sm-6 ">
                                        <div class="form-group">
                                            <label class="control-label">Middle Name</label>
                                            <input type="text" name="middleName" formControlName="middleName" class="form-control">
                                        </div>
                                    </div>

                                <div class="col-xs-6 col-md-4 col-sm-6  ">
                                        <div class="form-group">
                                            <label class="control-label"> <small class="AsteriskColor">* </small>First
                                                Name</label>
                                            <input type="text" name="firstName" formControlName="firstName" class="form-control">
                                            <ehr-field-error-display [displayError]="isFieldValid(updatePatientForm, 'firstName')"
                                                errorMsg="First Name is required">
                                            </ehr-field-error-display>
                                        </div>
                                    </div>
                                </div>
                                <div class="row adjust-content">
                                    <div class="col-md-1 col-xs-6 col-sm-6" style="top:7px">
                                        <div class="form-group">
                                            <label class="control-label"><small class="AsteriskColor">*</small> Sex
                                            </label>
                                            <md2-select name="gender" formControlName="gender" [(ngModel)]="patientGender_selectedValue"
                                                style="width: 100%">
                                             
                                                <md2-option value="male">Male</md2-option>
                                                <md2-option value="female">Female</md2-option>
                                                <md2-option value="unknown">Unknown</md2-option>
                                            </md2-select>
                                            <div class="error-display-margin">
                                                <ehr-field-error-display [displayError]="isFieldValid(updatePatientForm, 'gender')"
                                                    errorMsg="Required">
                                                </ehr-field-error-display>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="col-md-2 col-xs-6 col-sm-6" style="top:-9px;">
                                        <div class="form-group" >
                                            <label class="control-label"> <small class="AsteriskColor">*</small>  DOB       
                                            </label>
                                            <mat-form-field style="width: 100% !important;">
                                                <div class="input-icon-wrap">
                                                    <input matInput type="text" class="input-with-icon" id="dob" name="dob" formControlName="dob" readonly required>
                                                    <span class="input-icon" (click)="datepicker('dob')"><span class="fa fa-calendar fa-lg"></span></span>
                                                </div>
                                            </mat-form-field>
                                        </div>
                                        <div style="margin-top: -15px;">
                                            <ehr-field-error-display [displayError]="isFieldValid(updatePatientForm, 'dob')" errorMsg="Please select DOB">
                                            </ehr-field-error-display>
                                        </div>
                                    </div>
                                     <div class="col-md-1 col-xs-6  col-sm-6 ">
                                        <div class="form-group ">
                                            <label  class="control-label">
                                                <small class="AsteriskColor">*</small> Age
                                            </label>
                                            <input name="age" type="text" formControlName="age" readonly class="form-control">

                                        </div>
                                    </div>
                                    <div class="col-md-4 col-xs-6  col-sm-6 ">
                                        <div class="form-group">
                                            <label class="control-label"><small class="AsteriskColor">*</small> Phone No.
                                            </label>
                                            <input name="primaryNo" id="primaryNo" formControlName="primaryNo" type="text"
                                                 class="form-control">
                                            <ehr-field-error-display [displayError]="isFieldValid(updatePatientForm, 'primaryNo')"
                                                errorMsg="Please enter valid Phone no.(XXX-XXX-XXXX)">
                                            </ehr-field-error-display>
                                        </div>
                                    </div>
                                    <div class="col-md-4 col-xs-6  col-sm-6 ">
                                        <div class="form-group">
                                            <label class="control-label">Mobile No.
                                            </label>
                                            <input name="secondaryNo" id="secondaryNo" formControlName="secondaryNo" type="text" 
                                            class="form-control" >
                                            <ehr-field-error-display [displayError]="isFieldValid(updatePatientForm, 'secondaryNo')"
                                            errorMsg="Please enter valid Mobile no.(XXX-XXX-XXXX)">
                                        </ehr-field-error-display>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="col-md-12">
                            <div class="row adjust-content">
                            <div class="col-md-3 col-xs-6  col-sm-6 ">
                                        <div class="form-group">
                                            <label class="control-label"><small class="AsteriskColor">*</small> Email
                                            </label>
                                            <input name="primaryEmail" type="email" formControlName="primaryEmail"
                                                class="form-control">
                                            <ehr-field-error-display [displayError]="isFieldValid(updatePatientForm, 'primaryEmail')"
                                                errorMsg="Enter valid Email address">
                                            </ehr-field-error-display>
                                        </div>
                                </div>
                                <div class="col-md-3 col-xs-6 col-sm-6 ">
                                    <div class="form-group">
                                        <label class="control-label">Secondary Email </label>
                                        <input type="email" name="secondaryEmail" formControlName="secondaryEmail"
                                            class="form-control">
                                    </div>
                                </div>
                                 <div class="col-xs-6 col-md-3 col-sm-6 ">
                                    <div class="form-group">
                                        <label class="control-label">
                                            <small class="AsteriskColor">*</small> Social Security No.</label>
                                        <input type="text" id="ssn" formControlName="ssn" class="form-control">
                                     <ehr-field-error-display [displayError]="isFieldValid(updatePatientForm, 'ssn')"
                                            errorMsg="Please enter valid Social Security No.">
                                        </ehr-field-error-display>
                                    </div>
                                </div>
                                <div class="col-xs-6 col-md-3 col-sm-6 ">
                                    <div class="form-group">
                                        <label class="control-label">Driver License No. </label>
                                        <input type="text" name="licenseNo" formControlName="licenseNo" class="form-control">
                                    </div>
                                </div>
                             
                            </div>
                        </div>
                        <div class="col-md-12">
                            <div class="row adjust-content">
                                <div class="col-md-3 col-xs-6  col-sm-6 ">
                                    <div class="form-group">
                                        <label class="control-label"><small class="AsteriskColor">*</small> Street
                                            Name
                                        </label>
                                        <input name="patientAddressStreet" type="text" formControlName="patientAddressStreet"
                                            class="form-control ">
                                        <ehr-field-error-display [displayError]="isFieldValid(updatePatientForm, 'patientAddressStreet')"
                                            errorMsg="Please Enter Street Address.">
                                        </ehr-field-error-display>
                                    </div>
                                </div> 
                                 <div class="col-md-3 col-xs-6  col-sm-6 ">
                                    <div class="form-group">
                                        <label class="control-label">
                                            Building Number
                                        </label>
                                        <input type="text" id="buildingNo" formControlName="buildingNo" class="form-control">
                                        <ehr-field-error-display [displayError]="isFieldValid(updatePatientForm, 'buildingNo')" errorMsg="Please enter Building No">
                                        </ehr-field-error-display>
                                    </div>
                                </div>
                                <div class="col-sm-4 col-md-3 col-xs-4 ">
                                        <div class="form-group">
                                            <label class="control-label">Apartment No. </label>
                                            <input type="text" name="patientAddressDoorNo" formControlName="patientAddressDoorNo"
                                                class="form-control">
                                        </div>
                                </div> 
                                <div class="col-md-3 col-xs-6  col-sm-6 ">
                                        <div class="form-group">
                                            <label class="control-label"><small class="AsteriskColor">*</small> City
                                            </label>
                                            <input type="text" name="patientAddressCity" formControlName="patientAddressCity"
                                                class="form-control">
                                            <ehr-field-error-display [displayError]="isFieldValid(updatePatientForm, 'patientAddressCity')"
                                                errorMsg="Please enter city">
                                            </ehr-field-error-display>
                                        </div>
                                    </div>
                             
                            </div>
                        </div>
                         <div class="col-md-12">
                            <div class="row adjust-content">
                                <div class="col-md-3 col-xs-6 col-sm-6 " style="top:7px;">
                                    <div class="form-group">
                                        <label class="control-label"><small class="AsteriskColor">*</small> State
                                        </label>
                                        <md2-select (click)="clearVal()" name="patientAddressState" formControlName="patientAddressState"
                                            [(ngModel)]="patientAddressState_selectedValue" style="width: 100%">
                                            <md2-select-header>
                                                <input name="search" type="text" placeholder="Search for State"
                                                    [(ngModel)]="patientAddressState_search" [ngModelOptions]="{standalone: true}" />
                                            </md2-select-header>
                                            <md2-option class="decrease-height" *ngFor="let patientAddressState of allStates| lookupPipe : patientAddressState_search"
                                                [value]="patientAddressState.lookupCode">{{patientAddressState.lookupCode}}</md2-option>
                                        </md2-select>
                                    </div>
                                </div> 
                                <div class="col-md-3 col-xs-6  col-sm-6 ">
                                    <div class="form-group">
                                        <label class="control-label"><small class="AsteriskColor">*</small> ZIP Code
                                        </label>
                                        <input type="text" id="patientAddressZip" formControlName="patientAddressZip"
                                            class="form-control">
                                        <ehr-field-error-display [displayError]="isFieldValid(updatePatientForm, 'patientAddressZip')"
                                            errorMsg="Enter Zip Code in proper format (XXXXX)">
                                        </ehr-field-error-display>
                                    </div>
                                </div> 
                                <div class="col-sm-4 col-md-3 col-xs-4" style="top:7px;">
                                        <div class="form-group">
                                            <label class="control-label"><small class="AsteriskColor">*</small> Primary
                                                Provider
                                            </label>
                                            <md2-select (click)="clearVal()" name="staffId" formControlName="staffId"
                                                (change)="onProviderChange(staffId_selectedValue,primaryservicefloatLable)"
                                                [(ngModel)]="staffId_selectedValue" style="width: 100%">
                                                <md2-select-header>
                                                    <input name="search" type="text" placeholder="Search for Primary Provider"
                                                        [(ngModel)]="staffId_search" [ngModelOptions]="{standalone: true}" />
                                                </md2-select-header>
                                                <md2-option class="decrease-height" *ngFor="let staffId of allPrimaryProvider | primaryProvider : staffId_search"
                                                    [value]="staffId.staffId">{{staffId.staffFullName}}</md2-option>
                                            </md2-select>
                                            <div class="error-display-margin">
                                                <ehr-field-error-display [displayError]="isFieldValid(updatePatientForm, 'staffId')"
                                                    errorMsg="Please select Provider">
                                                </ehr-field-error-display>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="col-md-3 col-xs-6 col-sm-6" style="top:7px;">
                                            <div class="form-group">
                                                <label class="control-label">Primary
                                                    Service Location
                                                </label>
                                                <md2-select (click)="clearVal()" name="patientAddressId" formControlName="patientAddressId"
                                                    [(ngModel)]="patientAddressId_selectedValue" style="width: 100%">
                                                    <md2-select-header>
                                                        <input name="search" type="text" placeholder="Search for Primary Service Location"
                                                            [(ngModel)]="patientAddressId_search" [ngModelOptions]="{standalone: true}" />
                                                    </md2-select-header>
                                                    <md2-option class="decrease-height" *ngFor="let cliniclocation of allCliniclocations| primaryServiceLocation : patientAddressId_search"
                                                        [value]="cliniclocation.locationId">{{cliniclocation.locationName}}</md2-option>
                                                </md2-select>
                                            </div>
                                        </div>
                            </div>
                            </div>

                        <div class="col-md-12">
                            <div class="row adjust-content">
                              
                                <div class="col-md-3 col-xs-6 col-sm-6 " style="top:7px;">
                                        <div class="form-group">
                                            <label class="control-label">
                                                <small class="AsteriskColor">*</small> Preferred Language
                                            </label>
                                            <md2-select name="patientLanguage" formControlName="patientLanguage" [(ngModel)]="language_selectedValue"
                                                style="width: 100%">
    
                                                <md2-option class="decrease-height" *ngFor="let lang of allLanguages" [value]="lang.lookupCode">{{lang.lookupCode}}</md2-option>
                                            </md2-select>
                                            <div class="error-display-margin">
                                                <ehr-field-error-display [displayError]="isFieldValid(updatePatientForm, 'patientLanguage')" errorMsg="Please select Preferred Language">
                                                </ehr-field-error-display>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="col-md-3 col-xs-6  col-sm-6 " style="top:7px;">
                                            <div class="form-group">
                                                <label  class="control-label">
                                                    <small class="AsteriskColor">*</small> Race 
                                                </label>
                                                <md2-select name="race" formControlName="race" [(ngModel)]="race_selectedValue"
                                                    style="width: 100%">
        
                                                    <md2-option class="decrease-height" *ngFor="let rac of allRace" [value]="rac.lookupCode">{{rac.lookupCode}}</md2-option>
                                                </md2-select>
                                                <div class="error-display-margin">
                                                    <ehr-field-error-display [displayError]="isFieldValid(updatePatientForm, 'race')" errorMsg="Please select Race">
                                                    </ehr-field-error-display>
                                                </div>
                                            </div>
                                        </div>
                                        <div class="col-md-3 col-xs-6  col-sm-6 " style="top:6px">
                                                <div class="form-group">
                                                    <label class="control-label">
                                                        <small class="AsteriskColor">*</small> Ethnicity 
                                                    </label>
                                                    <md2-select name="ethnicity" formControlName="ethnicity" [(ngModel)]="ethnicity_selectedValue"
                                                        style="width: 100%">
            
                                                        <md2-option class="decrease-height" *ngFor="let ethnicity of allEthnicity" [value]="ethnicity.lookupCode">{{ethnicity.lookupCode}}</md2-option>
                                                    </md2-select>
                                                    <div class="error-display-margin">
                                                        <ehr-field-error-display [displayError]="isFieldValid(updatePatientForm, 'ethnicity')" errorMsg="Please select Ethnicity ">
                                                        </ehr-field-error-display>
                                                    </div>
                                                </div>
                                            </div>
                                <div class="col-md-3 col-xs-6  col-sm-6 " style="top:7px;">
                                    <div class="form-group">
                                        <label class="control-label"><small class="AsteriskColor">*</small> Patient
                                            Status
                                        </label>
                                        <md2-select (click)="clearVal()" name="additionalInfo" formControlName="additionalInfo"
                                            [(ngModel)]="additionalInfo_selectedValue" style="width: 100%">
                                            <md2-select-header>
                                                <input name="search" type="text" placeholder="Search for Patient Status"
                                                    [(ngModel)]="additionalInfo_search" [ngModelOptions]="{standalone: true}" />
                                            </md2-select-header>

                                            <md2-option class="decrease-height" *ngFor="let additionalInfo of alladditionalInfo | lookupPipe : additionalInfo_search"
                                                [value]="additionalInfo.lookupCode">{{additionalInfo.lookupCode}}</md2-option>
                                        </md2-select>
                                        <div class="error-display-margin">
                                            <ehr-field-error-display [displayError]="isFieldValid(updatePatientForm, 'additionalInfo')"
                                                errorMsg="Please select Patient Status">
                                            </ehr-field-error-display>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>

                         <div class="col-md-12" style="margin-left:57px;">
                            <a  (click)="showUpdateModal()">
                                <b>Update Patient Insurance Information</b>
                            </a>
                        </div>

  <div class="row" style="margin-left:57px;">
                            <div id="updateExpansionPanel" *ngIf="value">
                                <table class="table table-bordered">
                                    <tbody>
                                        <tr>
                                            <th class="text-center">Primary Insurance Information</th>
                                            <th class="text-center">Secondary Insurance Information</th>
                                        </tr>
                                        <tr>
                                            <td>
                                                <div class="row">
                                                    <div class="input-group">
                                                        <div>
                                                            <label>Patient Relationship to Insured</label>
                                                        </div>
                                                        <div>
                                                            <div>
                                                                <input name="PInsRelationship" formControlName="PInsRelationship" value="Self" type="radio" id="test1" />
                                                                <label for="test1">Self</label>
                                                            </div>
                                                            <div>
                                                                <input name="PInsRelationship" formControlName="PInsRelationship" value="Spouse" type="radio" id="test2" />
                                                                <label for="test2">Spouse</label>
                                                            </div>
                                                            <div>
                                                                <input name="PInsRelationship" formControlName="PInsRelationship" value="Child" type="radio" id="test3" />
                                                                <label for="test3">Child</label>
                                                            </div>
                                                            <div>
                                                                <input name="PInsRelationship" formControlName="PInsRelationship" value="Other" type="radio" id="test4" />
                                                                <label for="test4">Other</label>
                                                            </div>
                                                        </div>

                                                    </div>
                                                </div>
                                            </td>
                                            <td>
                                                <div class="row">
                                                    <div class="input-group">
                                                        <div class="">
                                                            <label>Patient Relationship to Insured</label>
                                                        </div>
                                                        <div>
                                                            <div>
                                                                <input name="SInsRelationship" formControlName="SInsRelationship" value="Self" type="radio" id="test1" />
                                                                <label for="test1">Self</label>
                                                            </div>
                                                            <div>
                                                                <input name="SInsRelationship" formControlName="SInsRelationship" value="Spouse" type="radio" id="test2" />
                                                                <label for="test2">Spouse</label>
                                                            </div>
                                                            <div>
                                                                <input name="SInsRelationship" formControlName="SInsRelationship" value="Child" type="radio" id="test3" />
                                                                <label for="test3">Child</label>
                                                            </div>
                                                            <div>
                                                                <input name="SInsRelationship" formControlName="SInsRelationship" value="Other" type="radio" id="test4" />
                                                                <label for="test4">Other</label>
                                                            </div>
                                                        </div>

                                                    </div>
                                                </div>
                                            </td>
                                        </tr>
                                        <tr>
                                            <td>
                                                <div class="col-md-4">
                                                    <label class="labelinsurance">Name of Insured:</label>
                                                </div>
                                                <div class="col-md-7">
                                                    <input type="text" name="PrimaryInsuredName" class="form-control" formControlName="PInsName">
                                                   <ehr-field-error-display [displayError]="isFieldValid(updatePatientForm, 'PInsName')" errorMsg="Please Enter Name of Insured">
                                                  </ehr-field-error-display>
                                                </div>
                                            </td>
                                            <td>
                                                <div class="col-md-4">
                                                    <label class="labelinsurance">Name of Insured:</label>
                                                </div>
                                                <div class="col-md-7">
                                                    <input type="text" name="SecondaryInsuredName" class="form-control" formControlName="SInsName">
                                                </div>
                                            </td>
                                        </tr>
                                        <tr>
                                            <td>
                                                <div class="col-md-4">
                                                    <label class="labelinsurance">Insured DOB:</label>
                                                </div>
                                                <div class="col-md-7">
                                                    <mat-form-field style="width: 100% !important;">
                                                        <div class="input-icon-wrap" style="margin-top:-8px;">
                                                            <input matInput type="text" class="input-with-icon" (click)="datepicker('PInsDob')"  id="PInsDob" name="PInsDob" formControlName="PInsDob" readonly
                                                                >
                                                            <span class="input-icon" (click)="datepicker('PInsDob')">
                                                                <span class="fa fa-calendar fa-lg"></span>
                                                            </span>
                                                        </div>
                                                         <ehr-field-error-display [displayError]="isFieldValid(updatePatientForm, 'PInsDob')" errorMsg="Please Enter Insured DOB">
                                                  </ehr-field-error-display>
                                                    </mat-form-field>
                                                </div>
                                            </td>
                                            <td>
                                                <div class="col-md-4">
                                                    <label class="labelinsurance">Insured DOB:</label>
                                                </div>
                                                <div class="col-md-7">
                                                    <mat-form-field style="width: 100% !important;">
                                                        <div class="input-icon-wrap" style="margin-top:-8px;">
                                                            <input matInput type="text" class="input-with-icon" (click)="datepicker('SInsDob')" readonly id="SInsDob" name="SInsDob" formControlName="SInsDob"
                                                                readonly>
                                                            <span class="input-icon" (click)="datepicker('SInsDob')">
                                                                <span class="fa fa-calendar fa-lg"></span>
                                                            </span>
                                                        </div>
                                                    </mat-form-field>
                                                </div>
                                            </td>
                                        </tr>
                                        <tr>
                                            <td>
                                                <div class="col-md-4">
                                                    <label class="labelinsurance">Insured Social Security:</label>
                                                </div>
                                                <div class="col-md-7">
                                                    <input type="text" name="PInsSsn" id="PInsSsn" formControlName="PInsSsn" class="form-control">
                                                   <ehr-field-error-display [displayError]="isFieldValid(updatePatientForm, 'PInsSsn')" errorMsg="Please Enter Insured Social Security No.">
                                                  </ehr-field-error-display>
                                                </div>
                                            </td>
                                            <td>
                                                <div class="col-md-4">
                                                    <label class="labelinsurance">Insured Social Security:</label>
                                                </div>
                                                <div class="col-md-7">
                                                    <input type="text" name="SInsSsn" id="SInsSsn" formControlName="SInsSsn" class="form-control">
                                                </div>

                                            </td>
