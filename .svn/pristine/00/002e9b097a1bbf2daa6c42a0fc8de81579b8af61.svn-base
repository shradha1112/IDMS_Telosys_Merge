    {"ICD10Code":"F55.1","Description":"Abuse of herbal or folk remedies","SearchInclusion":"Abuse of herbal medicine or folk remedies\nAbuse of herbal medicine or folk remedy\nHerbal or folk remedies abuse","ClinicalNotes":""},
    {"ICD10Code":"F55.2","Description":"Abuse of laxatives","SearchInclusion":"Chronic abuse of laxatives\nChronic laxative abuse\nLaxative abuse\nLaxative abuse, chronic","ClinicalNotes":""},
    {"ICD10Code":"F55.3","Description":"Abuse of steroids or hormones","SearchInclusion":"Abuse of steroids\nSteroid abuse","ClinicalNotes":""},
    {"ICD10Code":"F55.4","Description":"Abuse of vitamins","SearchInclusion":"Vitamin abuse","ClinicalNotes":""},
