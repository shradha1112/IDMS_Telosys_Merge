/* checkbox style */
/* common for addencounter, hpi, ros, history  */
input[type=checkbox] + label {
  display: block;
  margin: 0.9em;
  cursor: pointer;
  padding-left: 4.9em;
  }
  
  input[type=checkbox] {
  display: none;
  }
  
  input[type=checkbox] + label:before {
  content: "\2714";
  border: 0.1em solid #000;
  border-radius: 0.2em;
  display: inline-block;
  width: 1em;
  height: 0.0em;
  padding-left: 0.0em;
  padding-bottom: 1em;
  color: transparent;
  transition: .2s;
  }
  
  input[type=checkbox] + label:active:before {
  transform: scale(0);
  }
  
  input[type=checkbox]:checked + label:before {
  background-color: #0480c8;
  border-color: #0480c8;
  color: #fff;
  }
  
  input[type=checkbox]:disabled + label:before {
  transform: scale(1);
  border-color: #aaa;
  }
  
  input[type=checkbox]:checked:disabled + label:before {
  transform: scale(1);
  background-color: #bfb;
  border-color: #bfb;
  }

