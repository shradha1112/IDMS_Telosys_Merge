    <div class="modal fade" id="AddIcd" data-keyboard="false" data-backdrop="static">
        <div class="modal-dialog modal-lg" style="width:95%;top:-5%">
            <div class="card">
                <div class="modal-content">
                    <div class="modal-header text-center">
                        <h6 class="card-header card-style" data-background-color>ICD-10 Search</h6>
                        <button type="button" class="close pull-right" data-dismiss="modal">&times;</button>
                    </div>
                    <form [formGroup]="AddIcdForm" action="" method="">
                        <div class="row">
                            <div class="col-md-4">
                                <div class="btn-group btn-group-justified">
                                    <a data-toggle="tab" id="system1" (click)="viewMode='Frequently'"
                                        class="btn tab-btn-style"  [ngClass]="{'colorchange':viewMode=='Frequently'}">Frequently
                                        Used</a>
                                    <a data-toggle="tab" id="system2" (click)="viewMode='Others'"
                                        class="btn tab-btn-style"  [ngClass]="{'colorchange':viewMode=='Others'}">Others</a>
                                </div>
                                <div [ngSwitch]="viewMode">
                                <div id="tab1" *ngSwitchCase="'Frequently'">
                                    <div class="btn-group-vertical">
                                        <ng-container *ngFor="let rows of ICD10Group;let i=index">
                                            <a *ngIf="i<11" data-toggle="tab" id={{i}} (click)="onClicking(rows)"
                                                class="btn btn-style btn-left btn-content-style"  [ngClass]="{'colorchange':rows.groupId==groupId}"
                                                data-toggle="modal">{{rows.groupDescription}}<span *ngIf="rows.count!=0"
                                                    id="badge_{{i}}" class="badge badge-style"  [ngClass]="{'badge-style-change':rows.groupId !==groupId}">{{rows.count}}</span>
                                            </a>
                                        </ng-container>
                                    </div>
                                </div>
                                <div id="tab2" *ngSwitchCase="'Others'">
                                    <div class="btn-group-vertical ">
                                        <ng-container *ngFor="let rows of ICD10Group;let i=index">
                                            <a *ngIf="i>=11" data-toggle="tab" id={{i}} (click)="onClicking(rows)"
                                                class="btn btn-style  btn-left btn-content-style" [ngClass]="{'colorchange':rows.groupId==groupId}"
                                                data-toggle="modal">{{rows.groupDescription}}
                                                <span *ngIf="rows.count!=''" id="badge_{{i}}"
                                                    class="badge badge-style" [ngClass]="{'badge-style-change':rows.groupId !==groupId}">{{rows.count}}</span></a>
                                        </ng-container>
                                    </div>
                                </div>
                                 </div>
                            </div>
                            <div class="col-md-8">
                                <div class="row">
                                    <br>
                                    <div class="row">
                                        <div class="col-md-8">
                                            <b>{{icdgroupName}}</b></div>
                                        <div class="col-md-4" [style.display]="showFlag?'block':'none'"><b> Number of
                                                matched Records: {{searchCount}}</b></div>
                                    </div><br>
                                    <div class="input-group">
                                        <div class="form-group label-floating">
                                            <input #searchRef type="text" [ngModelOptions]="{standalone: true}"
                                                placeholder="Type ICD-10 Codes/Description......"
                                                style="width:200px !important;" id="searchICD" [(ngModel)]="searchText"
                                                class="form-control">
                                        </div>
                                        <div *ngIf="icdPrimaryErrorFlag" class="EncErrorMsg-style">
                                            <div class="alert alert-danger alert-danger-style" role="alert">
                                                Please Select Primary ICD-10
                                            </div>
                                        </div>
                                        <div [style.display]="searchCount==0?'block':'none'"><b>No Records Found</b></div>
                                        <div [hidden]="flag">
                                            <table class="table table-bordered"
                                                style="overflow-y:auto;max-height:400px;display:block;">
                                                <tbody>
                                                    <tr>
                                                        <th class="text-center">Select</th>
                                                        <th class="text-center">ICD10</th>
                                                        <th class="text-center">Description</th>
                                                        <th class="text-center">Search Inclusion</th>
                                                        <th style="white-space:nowrap;"
                                                            title="Click the below icon to view full detail of ICD-10 Code">
                                                            Detail</th>
                                                        <th class="text-center"
                                                            title="Select below checkbox if ICD Code is primary ICD Code">
                                                            Primary
                                                            ICD-10?</th>
                                                    </tr>
                                                    <tr *ngFor='let data of finalData' style="height:auto">
                                                        <td class="text-center">
                                                            <input type="checkbox" id="icd_{{data.ICD10Code}}"
                                                                name="checkListItem" [checked]="isChecked(data.ICD10Code)"
                                                                (change)="onChange(data,icdgroupName, $event.target.checked)"
                                                                value="{{data}}"> <label
                                                                for="icd_{{data.ICD10Code}}"></label>
                                                        </td>
                                                        <td>{{data.ICD10Code}} </td>
                                                        <td> {{data.Description}} </td>
                                                        <td [innerHTML]=data.SearchInclusion></td>
                                                        <td class="text-center">
                                                            <a href="#" (click)="openSearchWindow(data.ICD10Code)"
                                                                data-toggle="modal" title="View Details">
                                                                <i class="fa fa-eye icon-color"></i>
                                                            </a>
                                                        </td>
                                                        <td class="text-center">
                                                            <input type="checkbox" name="primaryIcd"
                                                                id="primary_{{data.ICD10Code}}"
                                                                [checked]="isCheckedPrimary(data.ICD10Code)"
                                                                (change)="onChangePrimary(data.ICD10Code,$event.target.checked)"
                                                                value={{data.ICD10Code}}> <label
                                                                for="primary_{{data.ICD10Code}}"></label>
                                                        </td>
                                                    </tr>
                                                </tbody>
                                            </table>
                                            <br>
                                            <button type="submit" (click)="submitData()" id="AddClose" class="btn button"
                                                [disabled]="buttonDisabled">ADD & CLOSE</button>
                                            <button id="dismissmodal" style="display:none;" type="submit"
                                                class="btn btn-info pull-left" data-dismiss="modal"></button>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </form>
                    <br>
                </div>
            </div>
        </div>
    </div>
    
    
    <div class="modal fade" data-keyboard="false" data-backdrop="static" id="icd10Modal">
        <div class="modal-dialog modal-lg" style="width:85%">
            <div class="card">
                <div class="modal-content">
                    <div class="modal-header text-center">
                        <h6 class="card-header card-style " data-background-color>ICD10 Details</h6>
                        <button type="button" class="close pull-right" data-dismiss="modal">&times;</button>
                    </div>
                    <div class="table-content">
