<div class="main-content">
        <div class="container-fluid">
            <div class="row">
                <div class="col-sm-12">
                    <div class="card">
                        <div class="card-header" data-background-color>
                            <div class="row">
                                <div class="col-sm-6 col-md-5  col-xs-5">
                                    <tr>
                                        <td>
                                            <h6>{{title}} ENCOUNTER</h6>

                                        </td>
                                    </tr>
                                </div>
                            </div>
                        </div>
                        <ngx-spinner bdColor="transparent" loadingText="Loading" size="defaut" color="#03659D"
                            fullScreen="true" type="line-scale-pulse-out-rapid">
                        </ngx-spinner>
                        <div *ngIf="showListModal">
                            <div id="modalListEncounter" class="modal fade" data-keyboard="false" data-backdrop="static">
                                <div class="modal-dialog modal-lg">
                                    <div class="modal-content">
                                        <div class="row" style="padding-top: 1%">
                                            <label class="label-style col-md-2 col-md-offset-8">
                                                <b><a (click)="generatePDF($event)" id="exportPDF" role="button"
                                                        style="text-decoration: underline;color:#0471af !important;">Export
                                                        to PDF</a>
                                                </b>
                                            </label>
                                            <label class="label-style col-md-2">
                                                <b><a (click)="generatePDF($event)" id="printPDF" role="button"
                                                        style="text-decoration: underline;color:#0471af !important;">Print
                                                        PDF</a>
                                                </b>
                                            </label>
                                        </div>
                                        <ehr-listencounter [encounterDetails]=encounter [acuityDetails]=acuityData
                                            [patientRecords]=patientRecords></ehr-listencounter>
                                        <br>
                                        <div class="col-md-offset-10">
                                            <input type='button' class="btn button" (click)="closeListEncounterModal()"
                                                value="Close" data-dismiss="modal" />
                                        </div>
                                        <br>
                                    </div>
                                </div>
                            </div>
                        </div>
                         <!-- Preventive Care Modal -->
                        <div *ngIf="showPreventiveCareModal">
                            <div id="modalPreventiveCare" class="modal fade" data-keyboard="false" data-backdrop="static">
                                <div class="modal-dialog modal-lg" >
                                    <div class="modal-content">
                                        <div class="modal-header text-center">
                                            <h6 class="card-header card-style" data-background-color>Preventive Care</h6>
                                            <button type="button" class="close pull-right"
                                                data-dismiss="modal" (click)="closeMyModal()">&times;</button>
                                        </div>
                                        <br>
                                        <ehr-preventive #PreventiveComp [patientId]=patientId></ehr-preventive>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <!-- Consultant Notes modal -->
                        <div *ngIf="showConsultantNotesModal">
                            <div id="modalConsultantNotes" class="modal fade" data-keyboard="false" data-backdrop="static">
                                <div class="modal-dialog modal-lg" >
                                    <div class="modal-content">
                                            <div class="card-content">
                                        <div class="modal-header text-center">
                                            <h6 class="card-header card-style" data-background-color>Consultant Notes</h6>
                                            <button type="button" class="close pull-right"
                                                data-dismiss="modal" (click)="closeConsultantMyModal()">&times;</button>
                                        </div>
                                        <br>
                                        <ehr-consultantnotes #ConsultantComp [patientId]=patientId></ehr-consultantnotes>
                                    </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <!-- Lab result modal -->
                        <div *ngIf="showLabResultModal">
                            <div id="modalLabResult" class="modal fade" data-keyboard="false" data-backdrop="static">
                                <div class="modal-dialog modal-lg" >
                                    <div class="modal-content">
                                        <div class="modal-header text-center">
                                            <h6 class="card-header card-style" data-background-color>Laboratory Results</h6>
                                            <button type="button" class="close pull-right"
                                                data-dismiss="modal" (click)="closeLabResultMyModal()">&times;</button>
                                        </div>
                                        <br>
                                        <ehr-labresult #LabResultComp [patientId]=patientId></ehr-labresult>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div *ngIf="showOrdersModal">
                                <div id="modalOrders" class="modal fade" data-keyboard="false" data-backdrop="static">
                                    <div class="modal-dialog modal-lg" style="width:80%">
                                        <div class="modal-content">
                                            <div class="modal-header text-center">
                                                <h6 class="card-header card-style" data-background-color>Orders</h6>
                                                <button type="button" class="close pull-right"
                                                    data-dismiss="modal" (click)="closeOrdersModal()">&times;</button>
                                            </div>
                                            <br>
                                            <ehr-orders [encounterId]=encounterId [patientId]=patientId [pageName]=pageName ></ehr-orders>
                                        </div>
                                    </div>
                                </div>
                            </div>
